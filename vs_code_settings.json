{
  "breadcrumbs.enabled": false,
  "cursor.cpp.disabledLanguages": ["plaintext"],
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "editor.codeLens": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.foldingHighlight": false,
  "editor.fontFamily": "'Monaspace Neon', Consolas, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.fontLigatures": "'calt', 'liga', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', '-ss09'",
  "editor.fontWeight": "350",
  "editor.formatOnSave": true,
  "editor.guides.highlightActiveBracketPair": true,
  "editor.guides.highlightActiveIndentation": false,
  "editor.guides.indentation": false,
  "editor.lineHeight": 1.4,
  "editor.lineNumbers": "on",
  "editor.linkedEditing": true,
  "editor.matchBrackets": "always",
  "editor.minimap.autohide": true,
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.mouseWheelZoom": true,
  "editor.occurrencesHighlight": "off",
  "editor.padding.bottom": 20,
  "editor.padding.top": 20,
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "all",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": false,
  "explorer.compactFolders": false,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "remote.autoForwardPortsSource": "hybrid",
  "search.smartCase": true,
  "terminal.integrated.fontFamily": "Consolas, monospace",
  "terminal.integrated.fontSize": 16,
  "vim.foldfix": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-b>": false,
    "<C-c>": false,
    "<C-f>": false,
    "<C-p>": false,
    "<C-x>": false,
    "<C-z>": false
  },
  "vim.ignorecase": false,
  "vim.smartcase": false,
  "vim.useSystemClipboard": true,
  "vim.vimrc.enable": true,
  "vsicons.dontShowNewVersionMessage": true,
  "window.commandCenter": true,
  "window.restoreWindows": "none",
  "window.zoomLevel": 1.2,
  "workbench.colorCustomizations": {
    "editor.linkedEditingBackground": "#4f6aa3c5"
  },
  "workbench.colorTheme": "Atom One Dark",
  "workbench.editor.showTabs": "multiple",
  "workbench.iconTheme": "vscode-icons",
  "[python]": {
    "editor.defaultFormatter": "eeyore.yapf"
  },
  "zenMode.fullScreen": false
}
